{
  "name": "assistant-dashboards",
  "version": "2.17.0.0",
  "main": "index.ts",
  "license": "Apache-2.0",
  "scripts": {
    "osd": "node ../../scripts/osd",
    "build": "yarn plugin-helpers build",
    "test:jest": "../../node_modules/.bin/jest --config ./test/jest.config.js",
